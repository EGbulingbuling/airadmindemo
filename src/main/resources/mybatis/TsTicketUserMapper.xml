<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.eg.airadmindemo.mapper.TsTicketUserMapper">
    <insert id="insTicUse">
        insert into ts_ticket_user values(default,#{0},#{1})
    </insert>

    <delete id="delTicUse">
        delete from ts_ticket_user where ticket_id=#{0}
    </delete>
</mapper>