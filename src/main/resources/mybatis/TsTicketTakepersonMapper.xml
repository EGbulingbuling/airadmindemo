<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.eg.airadmindemo.mapper.TsTicketTakepersonMapper">
    <insert id="insTicTak">
        insert into ts_ticket_takepersion values(default,#{0},#{1})
    </insert>

    <delete id="delTicTak">
        delete from ts_ticket_takepersion where ticket_id=#{0}
    </delete>
</mapper>